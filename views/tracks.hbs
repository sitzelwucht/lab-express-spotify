<div class="container">
    <table>
    <thead>
        <th>Title</th>
        <th>Listen</th>
    </thead>
    <tbody>
          {{#each data}}
        <tr>
            <td>{{this.name}}</td>
            <td><audio controls src="{{this.preview_url}}"> </audio></td>
        </tr>
    {{/each}}
    </tbody>
</table>
</div>
